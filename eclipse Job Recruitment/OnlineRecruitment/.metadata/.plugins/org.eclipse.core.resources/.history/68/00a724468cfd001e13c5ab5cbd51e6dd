package com.example.OnlineRecruitment.Entities;

import jakarta.persistence.EmbeddedId;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Inheritance;
import jakarta.persistence.InheritanceType;
import jakarta.persistence.Lob;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;


@Entity
public class Graduate extends User{

	

	private int yearOfPassing;
	private float percentage;
	private String[] languages;
	private String[] skills;
	private String[] projects;
	private String fileName;
	@Lob
	private byte[] file;
	
	
	public Graduate(int yearOfPassing, float percentage, String[] languages, String[] skills
			, String[] projects,String name,String email,String password,
			String phone,String fileName,byte[] file) {
		super(name,email,password,phone);
		this.yearOfPassing = yearOfPassing;
		this.percentage = percentage;
		this.languages = languages;
		this.skills = skills;
		this.projects = projects;
		this.fileName = fileName;
		this.file = file;
	}
	


	public Graduate() {
		// TODO Auto-generated constructor stub
		super();
	}



	public int getYearOfPassing() {
		return yearOfPassing;
	}


	public void setYearOfPassing(int yearOfPassing) {
		this.yearOfPassing = yearOfPassing;
	}


	public float getPercentage() {
		return percentage;
	}


	public void setPercentage(float percentage) {
		this.percentage = percentage;
	}


	public String[] getLanguages() {
		return languages;
	}


	public void setLanguages(String[] languages) {
		this.languages = languages;
	}


	public String[] getSkills() {
		return skills;
	}


	public void setSkills(String[] skills) {
		this.skills = skills;
	}


	public String[] getProjects() {
		return projects;
	}


	public void setProjects(String[] projects) {
		this.projects = projects;
	}


	public String getFileName() {
		return fileName;
	}


	public void setFileName(String fileName) {
		this.fileName = fileName;
	}


	public byte[] getFile() {
		return file;
	}


	public void setFile(byte[] file) {
		this.file = file;
	}

	

}
